import java.util.Scanner;
public class Main {
    public static void main(String[]args){
        Scanner scanner=new Scanner(System.in);

        System.out.println("请输入一个整数,输入回车继续：\n");
        int n=scanner.nextInt();
        int c,d,m;
        int a=1,b=2;
        //原理：数学归纳法，出斐波那契额数列
        for(int i=0;i<n-1;i++){
            c=a+b;
            d=b+c;
            a=b;b=c;
        }
        System.out.println(a);



    }
}
